<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deneme"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_SETTINGS" >
    </uses-permission>
    <!-- Internet Permissions -->
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- Permission to write to external storage -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.deneme.Ana"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.deneme.MainActivity"
            android:label="@string/title_activity_ana"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name="com.example.deneme.Parse"
            android:label="@string/title_activity_parse" >
        </activity>
        <activity
            android:name="com.example.deneme.ImageAdapter"
            android:label="@string/title_activity_image_adapter" >
        </activity>
        <activity
            android:name="com.example.deneme.GridViewConfig"
            android:label="@string/title_activity_grid_view_config" >
        </activity>
        <activity
            android:name="com.example.deneme.AndroidXMLParsingActivity"
            android:label="@string/title_activity_android_xmlparsing" >
        </activity>
        <activity
            android:name="com.example.deneme.Datum"
            android:label="@string/title_activity_datum" >
        </activity>
    </application>

</manifest>